"VertexLitGeneric"
{
	"$baseTexture" "models/weapons/c_items/c_carnival_mallet_blue"
	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" ".05"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[1.25 .5 2]"
	
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "10"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "1.5"						// Boost for ambient cube component of rim lighting

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}