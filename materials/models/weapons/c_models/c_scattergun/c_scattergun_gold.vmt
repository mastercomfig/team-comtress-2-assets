"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_models/c_scattergun/c_scattergun_gold"
	"$phongexponenttexture" "models/weapons/c_models/c_scattergun/c_scattergun_exp"

	"$yellow" "0"
	
	"$basemapalphaphongmask" "1"
	"$envmap" "cubemaps/cubemap_gold001"	
	"$envmaptint" "[2.5 2.5 1.15]"
	
	"$phong" "1"
	"$phongexponent" "90"
	"$phongboost" "10"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.5 .5 3]"

	// Rim lighting parameters
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" "0.5"
	"$rimmask" "1"

	"360?$color2" "[ 0.5 0.5 0.5 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}