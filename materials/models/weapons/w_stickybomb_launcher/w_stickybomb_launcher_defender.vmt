"VertexlitGeneric"
{
	"$baseTexture" "models/weapons/w_stickybomb_launcher/w_stickybomb_launcher_defender"

	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" ".75"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$basemapalphaphongmask" "1"
	"$glowcolor" "1"

	"$basephongexponent" "20"
	"$basephongboost" ".75"

	"$rimlight" "1"
	"$rimlightexponent" "25"	
	"$rimlightboost" ".125"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}