"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_models/c_scattergun/c_scattergun"
	"$phongexponenttexture" "models/weapons/c_models/c_scattergun/c_scattergun_exp"
	
	"$phong" "1"
	//commented out to use the exponent mask
	//"$phongexponent" "255"
	"$phongboost" "6"	
	
	//scales up the exponent texture which usually only has about 120 levels of exponent
	"$phongexponentfactor"  "195"
	"$phongalbedotint" "1"
	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
			
	"$phongfresnelranges"	"[.3 .5 3]"
	
	"$glowcolor" "1"
	
	"$envmap" "env_cubemap"
	"$envmaptint" "[.1 .07 .04]"
	
	"$selfillum" 1	
	"$selfillumtint" "[5 3 .7]"
  	"$selfIllumFresnel" "1"
  	"$selfIllumFresnelMinMaxExp" "[0 5 10]"
  
  	"$phongtint" "[.5 .4 .4]"
	
	

	//commented out to use the exponent mask
	//"$basephongexponent" "15"
	//"$basephongboost" "3"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "1"				
	"$rimlightboost" "1.5"
	"$rimmask" "1"
	

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
		"WeaponSkin" {
		}
	}
}