"VertexLitGeneric"
{
	"$baseTexture" "models/weapons/c_models/c_grenadelauncher/c_grenadelauncher"
	"$phongexponenttexture" "models/weapons/c_models/c_grenadelauncher/c_grenadelauncher_exp"
	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongexponent" "30"
	"$phongboost" "10"
	"$phongfresnelranges" "[0.45 0.7 1]"

	
	"$glowcolor" "1"

  	"$phongtint" "[.5 .4 .4]"

	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"

    // Rim lighting parameters
    	"$rimlight" "1"						
	"$rimlightexponent" "10"				
	"$rimlightboost" "1.5"
	"$rimmask" "1"

	"$envmap" "cubemaps\cubemap_specular001"
	"$envmaptint" "[.1 .1 .1]"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"WeaponSkin"
		{
		}
	}
}