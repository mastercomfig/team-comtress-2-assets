"VertexLitGeneric"
{
	"$basetexture" "models/weapons/c_models/c_minigun/c_minigun"
	"$phongexponenttexture" "models/weapons/c_models/c_minigun/c_minigun_exp"
	
	"$basemapalphaphongmask" "1"
	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" "3.5"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.55 .5 1]"

	

	"$basephongexponent" "20"
	"$basephongboost" "2"

	"$commadd_phongexponent" "30"
	"$commadd_phongboost" 	"22.5"
	"$commweapon"		"0"

	"$glowcolor" "1"
	
	"$selfillum" 1	
	"$selfillumtint" "[5 3 .7]"
  	"$selfIllumFresnel" "1"
  	"$selfIllumFresnelMinMaxExp" "[0 1.5 55]"
	
	// Rim lighting parameters
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" "3"
	"$rimmask" "1"	
	
  	"$phongtint" "[.5 .4 .4]"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"WeaponSkin"
		{
		}
	}
}